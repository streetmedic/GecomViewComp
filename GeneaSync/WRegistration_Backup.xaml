<Window x:Class="GedcomViewCompare.WRegistration_Backup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Product Registration" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="160" Width="300" Background="#FFE0E0EC">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Name="spacerLeft" Width="10" />
            <ColumnDefinition Name="content" Width="*" />
            <ColumnDefinition Name="spacerRight" Width="10" />
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="1" Height="20" HorizontalAlignment="Center" Margin="0,56,0,0" Name="textBox1" VerticalAlignment="Top" Width="258" />
        <Button Content="Register" Grid.Column="1" Height="22" HorizontalAlignment="Center" Margin="2,85,0,0" Name="buttonRegister" VerticalAlignment="Top" Width="118" IsDefault="True" />
        <TextBlock Grid.Column="1" Height="42" HorizontalAlignment="Left" Margin="0,24,0,0" Name="textBlock1" Text="Enter your registration code in the field below and click the Register button" VerticalAlignment="Top" TextWrapping="Wrap" />
    </Grid>
</Window>
